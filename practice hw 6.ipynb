   "source": [
    "def fahrenheit_celcius (fahrenheit):\n",
    "    celcius = (fahrenheit - 32) * 5/9\n",
    "    return celcius\n",
    "\n",
    "f = float ( input ('Input your degrees fahrenheit: ') )\n",
    "\n",
    "fahrenheit_celcius (f)"
